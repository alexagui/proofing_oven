%tr
  %td= link_to candidate.full_name, [:edit, candidate]
  %td= candidate.contact_phone
  %td= candidate.email
  %td= candidate.github_id
  %td= candidate.twitter_id
  %td= candidate.skills * ', '
  %td= link_to_if candidate.resume.present?, candidate.resume_filename, candidate.resume_url
  %td= button_to 'Accept', [:accept, candidate], method: :post
