= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
  %h1 Sign In
  = devise_error_messages!
  %fieldset#sign_in
    %dl
      %dt
        = f.label :email
      %dd
        = f.text_field :email
      %dt
        = f.label :password
      %dd
        = f.password_field :password
  %fieldset.actions
    = f.submit "Sign In"
    %p
      = link_to 'Sign Up', '#'
      = link_to 'Forgot Password', '#'
